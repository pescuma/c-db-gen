struct Produto
{
	Char codigo[16];
	Char nome[81];
	Char marca[81];
	Char embalagem[32];
	Char qtdeEmbalagem[12];
	Boolean feminino;
	Int32 preco;
	Int32 qtde;
	UInt8 flags | SOLD | REQUESTED ;
};

struct SellItem
{
	Produto produto;
	Int32 qtde;
	Int32 precoTotal;
};

struct Sell
{
	UInt32 client;
	UInt8 flags;
	List<SellItem> items;
};
